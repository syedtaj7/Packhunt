{
  "name": "packhunt-server",
  "version": "1.0.0",
  "description": "Backend API for PackHunt - Package discovery platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "tsx prisma/seed.ts",
    "import": "tsx src/scripts/import-packages.ts",
    "discover": "tsx src/scripts/auto-discover-packages.ts",
    "import-discovered": "tsx src/scripts/import-discovered.ts",
    "auto-update": "npm run discover && npm run import-discovered && npm run meilisearch:sync && npx tsx src/scripts/generate-embeddings.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "meilisearch:sync": "tsx src/scripts/sync-meilisearch.ts",
    "embeddings:generate": "tsx src/scripts/generate-embeddings.ts"
  },
  "keywords": ["api", "express", "packages", "search"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@xenova/transformers": "^2.17.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "meilisearch": "^0.55.0",
    "openai": "^6.16.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/node": "^22.10.5",
    "nodemon": "^3.1.9",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
